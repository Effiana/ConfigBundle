{
	"name": "craue/config-bundle",
	"type": "symfony-bundle",
	"description": "Database-stored settings made available via a service for your Symfony2 project.",
	"keywords": ["symfony2", "config"],
	"homepage": "https://github.com/craue/CraueConfigBundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Christian Raue",
			"email": "christian.raue@gmail.com"
		},
		{
			"name": "Symfony Community",
			"homepage": "https://github.com/craue/CraueConfigBundle/contributors"
		}
	],
	"require": {
		"php": ">=5.3.2",
		"symfony/framework-bundle": "~2.3|~3.0"
	},
	"require-dev": {
		"doctrine/orm": "~2.3",
		"doctrine/doctrine-bundle": "~1.0",
		"phpunit/phpunit": "~4.1",
		"symfony/assetic-bundle": "~2.3",
		"symfony/symfony": "~2.3|~3.0"
	},
	"minimum-stability": "stable",
	"autoload": {
		"psr-4": {
			"Craue\\ConfigBundle\\": ""
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.4.x-dev"
		}
	}
}
